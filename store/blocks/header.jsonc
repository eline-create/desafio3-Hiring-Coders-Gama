      "type": "internal",
      "href": "#",
      "noFollow": false,
      "tagTitle": "Desafios",
      "text": "DESAFIOS"
    },
    "iconProps": {
      "id": "mpa-filter--filled",
      "size": 16,
      "viewBox": "0 0 16 16",
      "activeClassName": "rebel-pink",
      "mutedClassName": "c-action-primary"
    }
  }
},
"menu-item#loja": {
  "props": {
    "id": "menu-item-loja",
    "type": "custom",
    "highlight": false,
    "itemProps": {
      "type": "internal",
      "href": "#",
      "noFollow": false,
      "tagTitle": "Loja",
      "text": "LOJA"
    },
    "iconProps": {
      "id": "hpa-cart",
      "size": 16,
      "viewBox": "0 0 16 16",
      "activeClassName": "rebel-pink",
      "mutedClassName": "c-action-primary"
    }
  }
},
  "menu-item#whatsapp": {
